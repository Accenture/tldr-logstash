NGINX_ACCESS \[%{HTTPDATE:nginx_timestamp}\] remote_addr=%{IPORHOST:nginx_remote_addr} server_name=%{IPORHOST:nginx_domain} upstream_addr=%{DATA:upstream_addr} request=\"(?:%{WORD:verb} %{NOTSPACE:request}(?: HTTP/%{NUMBER:httpversion})?|%{DATA:rawrequest})\" status=%{NUMBER:status} http_user_agent=%{DATA:http_user_agent} todo_request_id=\"%{DATA:todo_request_id}\" todo_session_id=\"%{DATA:todo_session_id}\" todo_client_id=\"%{DATA:todo_request_id}\" upstream_response_time=%{DATA:upstream_response_time} ms=%{DATA:ms} request_time=%{NUMBER:request_time:float}

NGINX_ERROR (?<nginx_timestamp>%{YEAR}[./-]%{MONTHNUM}[./-]%{MONTHDAY}[- ]%{TIME}) \[%{LOGLEVEL:severity}\] %{POSINT:pid}#%{NUMBER}: %{GREEDYDATA:errormessage}(?:, client: (?<client>%{IP}|%{HOSTNAME}))(?:, server: %{IPORHOST:server})(?:, request: %{QS:request})?(?:, host: %{QS:host})